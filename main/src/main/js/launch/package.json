{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://micronaut.io/launch",
    "dependencies": {
        "@material-ui/core": "^4.11.4",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.58",
        "materialize-css": "^1.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-materialize": "^3.9.8",
        "react-syntax-highlighter": "^15.4.4",
        "recoil": "^0.3.1"
    },
    "devDependencies": {
        "prettier": "^2.3.0",
        "react-scripts": "^4.0.3",
        "react-test-renderer": "^17.0.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "start:local": "export REACT_APP_API_URL=http://localhost:8080; export REACT_APP_SNAPSHOT_API_URL=http://localhost:8081; export REACT_APP_NEXT_API_URL=http://localhost:8082; export REACT_APP_PREV_API_URL=http://localhost:8083; export CORS_ALLOWED_ORIGIN=http://localhost:3000; npm run start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
        "lint": "eslint './src/**/*.js'",
        "lint:fix": "eslint './src/**/*.js' --fix"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
